<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.pruebas.proyectolenguajes.RegistroActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/registroLabelTitulo"
        android:id="@+id/textView2"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/registroLabelNombre"
        android:id="@+id/textView6"
        android:layout_marginTop="35dp"
        android:layout_below="@+id/textView2"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/registoUsuarioEditTexNombre"
        android:layout_above="@+id/registroTextView1Espacio"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/textView2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/registroLabelUser"
        android:id="@+id/textView7"
        android:layout_below="@+id/registroTextView1Espacio"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPassword"
        android:ems="10"
        android:id="@+id/registoUsuarioEditTextConstraseña"
        android:layout_alignBottom="@+id/textView8"
        android:layout_alignStart="@+id/registoUsuarioEditTexLogin" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/registroLabelContraseña"
        android:id="@+id/textView8"
        android:layout_below="@+id/registroTextView2Espacio"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/registoUsuarioEditTexLogin"
        android:layout_alignBottom="@+id/textView7"
        android:layout_alignStart="@+id/registoUsuarioEditTexNombre" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/registoUsuarioEditTextTelefono"
        android:layout_alignBottom="@+id/textView9"
        android:layout_alignStart="@+id/registoUsuarioEditTextConstraseña" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/registroLabelTelefono"
        android:id="@+id/textView9"
        android:layout_below="@+id/regisstroTextView3Espacio"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/registroButtonAceptar"
        android:id="@+id/registoUsuarioButtonAceptarUsuario"
        android:layout_centerVertical="true"
        android:layout_alignEnd="@+id/registoUsuarioEditTextTelefono" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/registroTextView4Espacio"
        android:layout_below="@+id/textView9"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/registroTextView1Espacio"
        android:layout_below="@+id/textView6"
        android:layout_toStartOf="@+id/registoUsuarioEditTexNombre" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/registroTextView2Espacio"
        android:layout_below="@+id/registoUsuarioEditTexLogin"
        android:layout_toStartOf="@+id/registoUsuarioEditTexNombre" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/regisstroTextView3Espacio"
        android:layout_below="@+id/textView8"
        android:layout_alignParentStart="true" />

</RelativeLayout>
