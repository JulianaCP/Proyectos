<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.pruebas.proyectolenguajes.AppAdminEditarProductosActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/appAdminLabelTituloEditarProducto"
        android:id="@+id/textView16"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/appAdminEditartextView17Espacio"
        android:layout_below="@+id/textView16"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/appAdminLabelNombreEditarProducto"
        android:id="@+id/textView18"
        android:layout_below="@+id/appAdminEditartextView17Espacio"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/appAdminEditartextView18Espacip"
        android:layout_below="@+id/textView18"
        android:layout_toEndOf="@+id/appAdminEditartextView17Espacio" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/appAdminLabelDescripcionEditarProducto"
        android:id="@+id/textView19"
        android:layout_below="@+id/appAdminEditartextView18Espacip"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/appAdminEditartextView19Espacio"
        android:layout_below="@+id/textView19"
        android:layout_alignEnd="@+id/textView19" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/appAdminLabelPrecioEditarProducto"
        android:id="@+id/textView20"
        android:layout_below="@+id/appAdminEditartextView19Espacio"
        android:layout_toEndOf="@+id/appAdminEditartextView17Espacio" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editTextNombreEditarProducto"
        android:layout_above="@+id/appAdminEditartextView18Espacip"
        android:layout_alignStart="@+id/textView16" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editTextDescripcionEditarProducto"
        android:layout_alignBottom="@+id/textView19"
        android:layout_alignStart="@+id/editTextNombreEditarProducto" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editTextPrecioEditarProducto"
        android:layout_alignBottom="@+id/textView20"
        android:layout_alignStart="@+id/editTextDescripcionEditarProducto" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/appAdminButtonGuardarEditarProducto"
        android:id="@+id/buttonAceptarEditarProducto"
        android:layout_marginTop="44dp"
        android:layout_below="@+id/textView20"
        android:layout_toEndOf="@+id/appAdminEditartextView18Espacip" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/appAdminButtonCancelarEditarProducto"
        android:id="@+id/buttonCancelarEditarProducto"
        android:layout_alignTop="@+id/buttonAceptarEditarProducto"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/textView23EspacioEditarProducto"
        android:layout_below="@+id/textView20"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/appAdminLabelCantidadEditarProducto"
        android:id="@+id/textView23"
        android:layout_below="@+id/textView23EspacioEditarProducto"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editTextCantidadEditarProducto"
        android:layout_alignBottom="@+id/textView23"
        android:layout_alignStart="@+id/editTextPrecioEditarProducto" />
</RelativeLayout>
