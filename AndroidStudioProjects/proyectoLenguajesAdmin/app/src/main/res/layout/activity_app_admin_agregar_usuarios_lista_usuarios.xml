<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.pruebas.proyectolenguajes.AppAdminAgregarUsuariosListaUsuariosActivity">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/registroLabelTitulo"
        android:id="@+id/textView2"
        android:layout_alignParentTop="true"
        android:layout_alignEnd="@+id/radioGroup"
        android:layout_marginEnd="37dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/registroLabelNombre"
        android:id="@+id/textView6"
        android:layout_below="@+id/regisstroTextView3Espacio"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/agregarUsuarioEditTexNombre"
        android:layout_alignBottom="@+id/textView6"
        android:layout_toEndOf="@+id/textView2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/registroLabelUser"
        android:id="@+id/textView7"
        android:layout_below="@+id/registroTextView4Espacio"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/agregarUsuarioEditTexLogin" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPassword"
        android:ems="10"
        android:id="@+id/agregarUsuarioEditTextConstraseña"
        android:layout_alignBottom="@+id/textView8"
        android:layout_toEndOf="@+id/textView2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/registroLabelContraseña"
        android:id="@+id/textView8"
        android:layout_below="@+id/registroTextView2Espacio"
        android:layout_alignParentStart="true"
        android:layout_marginTop="28dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/agregarUsuarioEditTexLogin"
        android:layout_alignBottom="@+id/textView7"
        android:layout_alignStart="@+id/agregarUsuarioEditTextTelefono" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/agregarUsuarioEditTextTelefono"
        android:layout_alignBottom="@+id/textView9"
        android:layout_alignStart="@+id/agregarUsuarioEditTexNombre" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/registroLabelTelefono"
        android:id="@+id/textView9"
        android:layout_below="@+id/registroTextView1Espacio"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/registroButtonAceptar"
        android:id="@+id/agregarUsuarioButtonAceptarUsuario"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/registroTextView4Espacio"
        android:layout_below="@+id/textView9"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/registroLabelUserType"
        android:id="@+id/textView11"
        android:layout_below="@+id/textView7"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/registroTextView1Espacio"
        android:layout_below="@+id/textView6"
        android:layout_toStartOf="@+id/editarEditTexNombre" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/registroTextView2Espacio"
        android:layout_below="@+id/editarEditTexFechaRegistro"
        android:layout_toStartOf="@+id/editarEditTexNombre" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/regisstroTextView3Espacio"
        android:layout_below="@+id/textView8"
        android:layout_alignParentStart="true" />

    <RadioGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/radioGroup"
        android:orientation="horizontal"
        android:layout_below="@+id/textView11"
        android:layout_alignParentStart="true">

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="onRadioButtonClickedUserType"
            android:text="@string/registroRadioButtonUserTypeCliente"
            android:id="@+id/agregarUsuarioOpcionCliente"
            android:checked="true" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="onRadioButtonClickedUserType"
            android:text="@string/registroRadioButtonUserTypeAdministrador"
            android:id="@+id/agregarUsuarioOpcionAdministrador"
            android:checked="false" />
    </RadioGroup>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignTop="@+id/radioGroup"
        android:layout_marginTop="51dp"
        android:id="@+id/linearLayout"
        android:layout_alignEnd="@+id/agregarUsuarioEditTexLogin"
        android:weightSum="1"
        android:layout_above="@+id/agregarUsuarioButtonAceptarUsuario">


    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/textView5"
        android:layout_below="@+id/textView7"
        android:layout_toEndOf="@+id/registroTextView4Espacio" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/agregarUsuarioContenedorAtributosAdministrador"
        android:layout_below="@+id/radioGroup"
        android:layout_alignEnd="@+id/agregarUsuarioButtonAceptarUsuario"
        android:layout_above="@+id/agregarUsuarioButtonAceptarUsuario">



        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/registroLabelDepartamento"
            android:id="@+id/textView14"
            android:layout_below="@+id/registroTextView5Espacio"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/agregarUsuarioEditTextDepartamento"
            android:layout_alignBottom="@+id/textView14"
            android:layout_alignParentEnd="true"
            android:layout_alignStart="@+id/agregarUsuarioEditTextSalario" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/registroTextView5Espacio"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/registroTextView6Espacio"
            android:layout_below="@+id/textView14"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/agregarUsuarioEditTextSalario"
            android:layout_alignBottom="@+id/textView50EspacioEditarUsuario"
            android:layout_toEndOf="@+id/textViewFEcha"
            android:layout_marginStart="135dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/registroLabelSalario"
            android:id="@+id/textView50EspacioEditarUsuario"
            android:layout_below="@+id/registroTextView6Espacio"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/registroTextView7Espacio"
            android:layout_below="@+id/textView50EspacioEditarUsuario"
            android:layout_toEndOf="@+id/registroTextView6Espacio" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/registroLabelCedula"
            android:id="@+id/textViewFEcha"
            android:layout_below="@+id/registroTextView7Espacio"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/agregarUsuarioEditTextCedula"
            android:layout_alignBottom="@+id/textViewFEcha"
            android:layout_alignStart="@+id/agregarUsuarioEditTextSalario" />
    </RelativeLayout>

</RelativeLayout>
